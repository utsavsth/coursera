@using MVCDemo.Models

@model IEnumerable<Subject>

@{
    ViewBag.Title = "Subject Lists";
}
@Html.ActionLink("Create", "Create")
<table>
    <tr>
        <th>Id
        </th>
        <th>Name
        </th>
        <th>Description
        </th>
    </tr>
    @if (@Model != null)
    {
        foreach (Subject subject in @Model)
        {
        <tr>
            <td>@subject.SubjectId</td>
            <td>@subject.Name</td>
            <td>@subject.Description</td>
            <td><a href="@Url.Action("Index", "Section", new { id = subject.SubjectId })">Show Section </a></td>
        </tr>
        }
    }
</table>
